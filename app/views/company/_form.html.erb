<%= render "error" %>
<%= form_for :company do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :nif %><br>
    <%= f.text_field :nif %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>
